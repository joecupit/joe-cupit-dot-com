---
import "../styles/wainwrights.css";

import Layout from "../layouts/Layout.astro";

const WainwrightCount = 92
---

<Layout title="wainwrights">
	<h1>Wainwrights</h1>

  <div class="progress" style=`--_rotation: ${((WainwrightCount / 214) * 360).toFixed(0)}deg;`>
    <div class="inner-progress">{WainwrightCount}<span>/214</span></div>
  </div>
</Layout>